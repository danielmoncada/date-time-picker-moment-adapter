{
  "name": "@danielmoncada/angular-datetime-picker-moment-adapter",
  "version": "6.0.0",
  "description": "Angular Date Time Picker (MomentJs Adapter)",
  "keywords": [
    "Angular",
    "datepicker",
    "date picker",
    "timepicker",
    "time picker",
    "datetime picker",
    "date time picker",
    "material",
    "ngx",
    "moment",
    "momentjs"
  ],
  "author": "Maintained and updated by Daniel Moncada, original implementatiom by Daniel Pan",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build picker",
    "test": "ng test picker",
    "test-with-coverage": "ng test --watch=false --no-progress --browsers=ChromeHeadlessNoSandbox --code-coverage=true picker",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build_lib": "ng build picker --configuration production && cp README.md dist/picker",
    "npm_pack": "cd dist/picker && npm pack",
    "package_windows": "npm run build_lib && npm run npm_pack",
    "package_linux": "npm run build_lib && npm run npm_pack"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/danielmoncada/date-time-picker-moment-adapter.git"
  },
  "dependencies": {
    "@angular/animations": "^19.0.0",
    "@angular/common": "^19.0.0",
    "@angular/compiler": "^19.0.0",
    "@angular/core": "^19.0.1",
    "@angular/forms": "^19.0.0",
    "@angular/platform-browser": "^19.0.0",
    "@angular/platform-browser-dynamic": "^19.0.0",
    "@angular/router": "^19.0.0",
    "moment": "^2.30.1",
    "moment-timezone": "^0.5.46",
    "np": "^8.0.4",
    "rxjs": "^7.8.1",
    "tslib": "^2.3.0",
    "zone.js": "^0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^19.0.1",
    "@angular/build": "^19.0.1",
    "@angular/cdk": "^19.0.1",
    "@angular/cli": "^19.0.2",
    "@angular/compiler-cli": "^19.0.1",
    "@angular/language-service": "^19.0.1",
    "@danielmoncada/angular-datetime-picker": "^19.0.0",
    "@types/jasmine": "^3.10.0",
    "@types/jasminewd2": "^2.0.10",
    "@types/moment-timezone": "^0.5.30",
    "@types/node": "^16.11.7",
    "codecov": "^3.6.5",
    "codelyzer": "^6.0.0",
    "jasmine-core": "~4.0.0",
    "jasmine-spec-reporter": "~7.0.0",
    "karma": "~6.3.4",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.7.0",
    "ng-packagr": "^19.0.1",
    "protractor": "~7.0.0",
    "ts-node": "^8.6.2",
    "tslint": "~6.1.0",
    "typescript": "^5.5.4"
  }
}

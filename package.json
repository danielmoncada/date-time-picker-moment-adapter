{
  "name": "@danielmoncada/angular-datetime-picker-moment-adapter",
  "version": "6.0.0",
  "description": "Angular Date Time Picker (MomentJs Adapter)",
  "keywords": [
    "Angular",
    "date picker",
    "date time picker",
    "datepicker",
    "datetime picker",
    "material",
    "moment",
    "momentjs",
    "ngx",
    "time picker",
    "timepicker"
  ],
  "author": "Maintained and updated by Daniel Moncada, original implementatiom by Daniel Pan",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build picker",
    "test": "ng test picker",
    "test-with-coverage": "ng test --watch=false --no-progress --browsers=ChromeHeadlessNoSandbox --code-coverage=true picker",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build_lib": "ng build picker --configuration production && cp README.md dist/picker",
    "npm_pack": "cd dist/picker && npm pack",
    "package_windows": "npm run build_lib && npm run npm_pack",
    "package_linux": "npm run build_lib && npm run npm_pack"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/danielmoncada/date-time-picker-moment-adapter.git"
  },
  "dependencies": {
    "@angular/animations": "^19.0.5",
    "@angular/common": "^19.0.5",
    "@angular/compiler": "^19.0.5",
    "@angular/core": "^19.0.5",
    "@angular/forms": "^19.0.5",
    "@angular/platform-browser": "^19.0.5",
    "@angular/platform-browser-dynamic": "^19.0.5",
    "@angular/router": "^19.0.5",
    "moment": "^2.30.1",
    "moment-timezone": "^0.5.46",
    "np": "^10.1.0",
    "rxjs": "^7.8.1",
    "tslib": "^2.8.1",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^19.0.6",
    "@angular/cdk": "^19.0.4",
    "@angular/cli": "^19.0.6",
    "@angular/compiler-cli": "^19.0.5",
    "@angular/language-service": "^19.0.5",
    "@danielmoncada/angular-datetime-picker": "^19.0.0",
    "@types/jasmine": "^5.1.5",
    "@types/jasminewd2": "^2.0.13",
    "@types/node": "^22.10.2",
    "angular-eslint": "19.0.2",
    "codecov": "^3.8.2",
    "eslint": "^9.17.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsdoc": "^50.6.1",
    "jasmine-core": "~5.5.0",
    "jasmine-spec-reporter": "~7.0.0",
    "karma": "~6.4.4",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage-istanbul-reporter": "~3.0.3",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "^2.1.0",
    "ng-packagr": "^19.0.1",
    "prettier": "3.4.2",
    "ts-node": "^10.9.2",
    "typescript": "~5.6.3",
    "typescript-eslint": "8.18.1"
  }
}
